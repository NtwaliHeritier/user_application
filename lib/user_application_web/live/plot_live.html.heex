<div class="w-1/4 mx-auto">
    <.form for={@form} phx-submit="save">
        <.input field={@form[:name]} placeholder="Name" required />
        <.input field={@form[:dataset_name]} placeholder="Dataset name" required />
        <.input field={@form[:expression]} placeholder="Expression" required />
        <.button type="submit" class="mt-3">Save</.button>
    </.form>
</div>